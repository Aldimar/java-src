package utility;

public class ContaCorrente
{

	String nome;
	String n_conta;
	double balance;

	double withdraw()
	{
		
		double withdraw_value = 0.0;
		return (saldo - withdraw_value);
		
	}

	double deposit()
	{
		
		double deposit_value = 0.0;
		return (saldo + deposit_value);
		
	}

	double consultarSaldo()
	{
		
		return saldo;
		
	}

	String consultarNome()
	{
		
		return nome;
		
	}
	
	String alterarNome() 
	{
		String newName = "";
		newName = nome;
		return newName;
	}

}

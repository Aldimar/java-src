package utility;

public class ContaCorrente
{

	String nome;
	String n_account;
	double balance;

	double withdraw()
	{
		
		double withdraw_value = 0.0;
		return (balance - withdraw_value);
		
	}

	double deposit()
	{
		
		double deposit_value = 0.0;
		return (balance + deposit_value);
		
	}

	double consultarbalance()
	{
		
		return balance;
		
	}

	String consultarNome()
	{
		
		return nome;
		
	}
	
	String alterarNome() 
	{
		String newName = "";
		newName = nome;
		return newName;
	}

}

 * 
 */
package utility;

/**
 * @author aldimar.junior
 *
 */
public class Calculadora
{

	public double sum(double x, double y) 
	{
		return sum(x + y);
	}
	
	public double sub(double x, double y) 
	{
		return sub();
	}
	
	public double div(double x, double y) 
	{
		return div();
	}
	
	public double mul(double x, double y) 
	{
		return mul();
	}
	
}

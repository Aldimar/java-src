package utility;

public class ContaCorrente
{

	String name;
	String n_account;
	double balance;

	double withdraw()
	{
		
		double withdraw_value = 0.0;
		return (balance - withdraw_value);
		
	}

	double deposit()
	{
		
		double deposit_value = 0.0;
		return (balance + deposit_value);
		
	}

	double checkBalance()
	{
		
		return balance;
		
	}

	String checkNome()
	{
		
		return name;
		
	}
	
	String checkNome() 
	{
		String newName = "";
		newName = name;
		return newName;
	}

}
